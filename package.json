      "name": "Marcela Rodriguez",
      "email": "marcela@cantina.co"
    }
  ],
  "engines": {
    "node": ">= 5.0.0"
  },
  "scripts": {
    "watch-js": "watchify player.js -o dist/player.js",
    "watch-css": "watchify -g browserify-css player-skin.js -o dist/player-skin.js -v",
    "watch": "npm run watch-js & npm run watch-css",
    "build-js": "browserify player.js > dist/player.js",
    "build-css": "browserify -g browserify-css player-skin.js > dist/player-skin.js",
    "build": "npm run build-js & npm run build-css",
    "serve": "npm run watch & nodemon ./server.js"
  },
  "devDependencies": {
    "browserify": "13.0.0",
    "browserify-css": "0.9.1",
    "express": "4.13.4",
    "express-http-proxy": "0.6.0",
    "native-promise-only": "0.8.1",
    "nodemon": "1.9.1",
    "three": "0.75.0",
    "ua-parser-js": "0.7.10",
    "video.js": "5.8.8",
    "watchify": "3.7.0",
    "webvr-boilerplate": "0.4.1",
    "webvr-polyfill": "0.9.2"
  }
}
